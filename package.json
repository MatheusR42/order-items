{
  "name": "order-items",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "lint": "eslint --ext js,jsx,ts,tsx .",
    "format": "prettier --write \"**/*.{ts,js,json}\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,js,tsx,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.json": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@types/node": "^14.6.2",
    "@vtex/prettier-config": "^0.3.1",
    "eslint": "^7.8.0",
    "eslint-config-vtex": "^12.8.6",
    "eslint-config-vtex-react": "^6.7.8",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.13",
    "prettier": "^2.1.1",
    "react": "^16.13.1",
    "typescript": "^4.0.2"
  }
}
